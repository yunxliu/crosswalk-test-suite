        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="manual" id="Manifest_Display_Standalone" purpose="Windows - Validate if web app can be launched with standalone">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/Manifest_Display_Standalone.html</test_script_entry>
        </description>
      </testcase>
