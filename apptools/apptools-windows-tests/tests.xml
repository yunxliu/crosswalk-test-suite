        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_platform_info" purpose="Windows - Validate if App Tool support instantiate platform backend and can print the info of platform" subcase="3">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/platform-info.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_platforms_manager" purpose="Windows - Validate if App Tool support load default backend for loaded platform" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/platforms-manager.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_template_file" purpose="Windows - Validate if App Tool support create/write/read a template file" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/template-file.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_test_util" purpose="Windows - Validate if a temp directory/file can be created under folder test-tmp" subcase="3">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/test-util.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_windows_main" purpose="Windows - Validate if project can be created with windows platforms" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/windows/test/main.js</test_script_entry>
        </description>
      </testcase>
    </set>
    <set name="CLI_manual" type="js">
      <testcase component="Crosswalk App Tools/CLI" execution_type="manual" id="AppTools_Crosswalk_App_Help" purpose="Windows - Validate if all usages of crosswalk-app work well as description">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/AppTools_Crosswalk_App_Help.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="manual" id="AppTools_Crosswalk_Pkg_Help" purpose="Windows - Validate if all usages of crosswalk-pkg work well as description">
