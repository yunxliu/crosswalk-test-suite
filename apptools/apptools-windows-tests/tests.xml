<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="Crosswalk App Tools" name="apptools-windows-tests">
    <set name="CLI" type="pyunit">
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_create_basic" purpose="Windows - Validate if project is created fail when the project has been exist">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/create_basic.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_pkgName" purpose="Windows - Validate project can be created with validate package naming rule" subcase="16">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/pkgName.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_build" purpose="Windows - Validate if project is built successfully with build command">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/build.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_update" purpose="Windows - Validate if project can be updated successfully with update command" subcase="5">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/update.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_app_version" purpose="Windows - Validate if crosswalk-app version command can display version information">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/crosswalk_app_version.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_pkg_version" purpose="Windows - Validate if crosswalk-pkg version command can display version information">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/crosswalk_pkg_version.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_target_platforms" purpose="Windows - Validate if platforms command can list available target platforms">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/target_platforms.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_build_path" purpose="Windows - Validate if project can be built with webapp path">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/build_path.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_manifest_package_id" purpose="Windows - Validate if package ID is used as expansion field" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/manifest_package_id.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_manifest_apk_name" purpose="Windows - Validate if apk name contains versionCode" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/manifest_apk_name.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_manifest_app_name" purpose="Windows - Validate if app name could contains whitespace">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/manifest_app_name.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_check_host" purpose="Windows - Validate if check command can test host setup" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/check_host.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_manifest_file_name" purpose="Windows - Validate if file name must not start with '.'" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/manifest_file_name.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_create_package" purpose="Windows - Validate if package can be created without a project" subcase="7">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/create_package.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_manifest_display" purpose="Windows - Validate if project can be built with valid display value" subcase="3">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/manifest_display.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_init_manifest" purpose="Windows - Validate if the web manifest can be initialised and updated" subcase="4">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/apptools/init_manifest.py</test_script_entry>
        </description>
      </testcase>
    </set>
    <set name="common_module" type="nodeunit">
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_application" purpose="Windows - Validate if the config and log message are correct when create a new project" subcase="11">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/application.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_finite_progress" purpose="Windows - Validate if a finite progress can be created" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/finite-progress.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_infinite_progress" purpose="Windows - Validate if a infinite progress can be created" subcase="2">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/infinite-progress.js</test_script_entry>
