      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_logfile_output" purpose="Windows - Validate if output all log message include error, warning and so on to logfile" subcase="7">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/logfile-output.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_manifest" purpose="Windows - Validate if app version and target Platforms are in manifest file" subcase="16">
        <description>
          <test_script_entry>/opt/apptools-windows-tests/tools/crosswalk-app-tools/test/manifest.js</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/Common Module" execution_type="auto" id="Crosswalk_output_tee" purpose="Windows - Validate if output all log message include error, warning and so on" subcase="7">
