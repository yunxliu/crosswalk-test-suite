<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite name="wrt-packagemgt-tizen-tests" category="Package_Management_on_Tizen" launcher="xwalk">
    <set name="packagemgt">
      <testcase purpose="Validate if web app icon display normally" type="Functional" status="designed" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_App_Icon">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure there some webapps are available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_App_Icon.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support that web app could be launched from app launcher successfully" type="Functional" status="designed" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_App_Icon_Launcher">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure there some webapps are available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_App_Icon_Launcher.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support web apps could be uninstalled successfully" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_WebApp_Uninstall">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Uninstall.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate Crosswalk support that web app could be launched successfully" type="Functional" status="designed" component="Package Management On Tizen" execution_type="manual" priority="P0" id="Crosswalk_WebApp_Launch">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched
            2.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate Crosswalk support that web app could be closed successfully" type="Functional" status="designed" component="Package Management On Tizen" execution_type="manual" priority="P0" id="Crosswalk_WebApp_Close">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched
            2.Make sure have one web app is running
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Close.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support multiple web apps could be launched successfully" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P2" id="Crosswalk_Multiple_Apps_Launch">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched
            2.Make sure multiple webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Multiple_Apps_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support to install Widget" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_Wiget_Install">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Install.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support to launch the installed Widget" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_Wiget_Launch">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support to Update the installed Widget" type="Functional" status="designed" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_Wiget_Update_SameVersion">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Update_SameVersion.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk support to Uninstall the installed Widget" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_Wiget_Uninstall">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Wiget_Uninstall.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate Crosswalk could debug by remote host" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_Remote_Debugging_Connection">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Remote_Debugging_Connection.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate Crosswalk viewed webpage could be modified by remote debug" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P2" id="Crosswalk_Remote_Debugging_CheckInfo">
        <description>
          <pre_condition>
            1.Make sure Crosswalk lanunched by remote debug mode.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Remote_Debugging_CheckInfo.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate Crosswalk viewed webpage could be modified by remote debug" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P2" id="Crosswalk_Remote_Debugging_Modification">
        <description>
          <pre_condition>
            1.Make sure Crosswalk lanunched by remote debug mode.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Remote_Debugging_Modification.html</test_script_entry>
        </description>
      </testcase>
        <testcase purpose="Check launch web app via command line" type="Functional" status="approved" component="Crosswalk/Runtime/Basicfeature" execution_type="manual" priority="P1" id="Crosswalk_WebApp_Launch_CommandLine">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed.
            1.Make sure webapp is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Launch_CommandLine.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate Crosswalk support different types of web apps" type="Functional" status="approved" component="Web App Management" execution_type="manual" priority="P2" id="Crosswalk_WebApp_Different_Types">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched;
            2.Make sure there some webapps are available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Different_Types.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if Crosswalk can check system memory" type="Functional" status="approved" component="Web App Management" execution_type="manual" priority="P2" id="Crosswalk_Check_System_Memory">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_Check_System_Memory.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if webapp folder is create after the webapp installed successfully" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_WebApp_Install_Resource">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Install_Resource.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if webapp cache file is deleted after the webapp uninstalled successfully" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_WebApp_Uninstall_Resource">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Uninstall_Resource.html</test_script_entry>
        </description>
      </testcase>
      <testcase purpose="Validate if webapp isolated partition cache is created after run the app" type="Functional" status="approved" component="Package Management On Tizen" execution_type="manual" priority="P1" id="Crosswalk_WebApp_Launcher_Cache">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgt-tizen-tests/packagemgt/Crosswalk_WebApp_Launcher_Cache.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
